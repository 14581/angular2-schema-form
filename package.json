  },
  "scripts": {
    "rimraf": "rimraf",
    "typedoc": "typedoc --target es6 --mode file --out ./doc/ ./src/index.ts ./typings/index.d.ts",
    "webpack": "webpack",
    "webdriver-manager": "webdriver-manager",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist",
    "clean:dist": "rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "webpack-dev-server": "webpack-dev-server --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --display-reasons",
    "tsc": "tsc",
    "prebuild": "npm run clean:dist",
    "test": "karma start",
    "typings": "typings",
    "start": "npm run webpack-dev-server",
    "start:hmr": "npm run server:dev:hmr",
    "protractor": "protractor",
    "prepublish": "export NODE_ENV=production; npm run build",
    "postinstall": "npm run typings -- install"
  },
  "typings": "./dist/src/index.d.ts",
